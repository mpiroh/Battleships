<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="1000dp" />
            <solid android:color="#1766A6" />
            <stroke
                android:width="2dip"
                android:color="#0E3F66" />
            <padding
                android:bottom="8dp"
                android:left="4dp"
                android:right="4dp"
                android:top="8dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="1000dp" />
            <solid android:color="#313F8C" />
            <stroke
                android:width="2dip"
                android:color="#000B4D" />
            <padding
                android:bottom="8dp"
                android:left="4dp"
                android:right="4dp"
                android:top="8dp" />
        </shape>
    </item>
</selector>